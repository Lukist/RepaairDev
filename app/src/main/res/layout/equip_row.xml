<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants">

    <ImageView
        android:id="@+id/equipRow_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/baseline_build_24"
        android:layout_margin="15dp"/>

    <TextView
        android:id="@+id/equipRow_serie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:textSize="15dp"
        android:layout_toRightOf="@id/equipRow_logo"
        />

    <TextView
        android:id="@+id/equipRow_tipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/equipRow_serie"
        android:layout_margin="15dp"
        android:textSize="15dp"
        />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:text="Borrar"
        android:layout_margin="5dp"
        android:backgroundTint="@color/black"/>



</RelativeLayout>